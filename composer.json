{
    "name": "zus1/laravel-serializer",
    "description": "Serializer library for Laravel",
    "keywords": ["larave", "serilaizer", "php", "json"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mario Ožuška",
            "email": "mario.ozuska@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/database": "^11.0",
        "illuminate/pagination": "^11.0",
        "illuminate/support": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Zus1\\Serializer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "Serializer": "Zus1\\Serializer\\Facades\\Serializer"
            },
            "providers": [
                "Zus1\\Serializer\\Providers\\LaravelSerializerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
